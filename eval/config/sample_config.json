{
    "features": [
        {
            "name": "Test Features",
            "path": "bioniceval/datasets/test_features.tsv",
            "delimiter": ","
        }
    ],
    "networks": [
        {
            "name": "Test Net",
            "path": "bioniceval/datasets/test_net.txt",
            "delimiter": " "
        }
    ],
    "standards": [
        {
            "name": "Test Coannotation Standard",
            "task": "coannotation",
            "path": "bioniceval/standards/test_coannotation_standard.txt",
            "delimiter": " "
        },
        {
            "name": "Test Cluster Standard",
            "task": "module_detection",
            "path": "bioniceval/standards/test_module_standard.pickle",
            "samples": 10,
            "methods": [
                "average",
                "complete"
            ],
            "metrics": [
                "cosine"
            ],
            "thresholds": 10
        },
        {
            "name": "Test Function Prediction",
            "task": "function_prediction",
            "path": "bioniceval/standards/test_function_standard.csv",
            "delimiter": ",",
            "test_size": 0.1,
            "folds": 5,
            "trials": 10,
            "gamma": {
                "minimum": 1e-6,
                "maximum": 1e-1,
                "samples": 10
            },
            "regularization": {
                "minimum": 1e-2,
                "maximum": 1e3,
                "samples": 50
            }
        }
    ],
    "consolidation": "union"
}